	"scripts": {
		"clean": "rimraf dist",
		"build": "pnpm run --silent clean && cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --env prod --fail-on-warnings",
		"dev": "pnpm run --silent clean && cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack serve",
		"lint": "eslint --max-warnings 0 --report-unused-disable-directives .",
		"lint:fix": "eslint --fix .",
		"type-check": "pnpm run '/type-check:.*/'",
		"type-check:test": "tsc -p ./test/tsconfig.json --noEmit",
		"type-check:src": "tsc -p ./tsconfig.json --noEmit",
		"type-check:webpack": "tsc -p ./tsconfig.webpack.json --noEmit",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"start": "pnpm dev"
	},
