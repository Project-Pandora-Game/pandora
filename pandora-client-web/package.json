		"delaunator": "5.0.1",
		"history": "5.3.0",
		"immer": "10.1.1",
		"inter-ui": "4.1.0",
		"lodash-es": "4.17.21",
		"lz-string": "1.5.0",
		"nanoid": "3.3.8",
		"pandora-common": "workspace:*",
		"pixi-viewport": "6.0.3",
		"pixi.js": "8.7.3",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-hook-form": "7.54.2",
		"react-reconciler": "0.31.0",
		"react-reverse-portal": "2.2.0",
		"react-rnd": "10.5.2",
		"react-router": "6.30.0",
