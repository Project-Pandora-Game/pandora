{
	"name": "pandora-client-web",
	"version": "0.1.0",
	"private": true,
	"repository": "git@github.com:Project-Pandora-Game/pandora-client-web.git",
	"license": "LGPL-3.0-or-later",
	"type": "module",
	"scripts": {
		"clean": "rimraf dist",
		"build": "pnpm run --silent type-check:webpack && pnpm run webpack:build",
		"dev": "pnpm run --silent type-check:webpack && pnpm run webpack:serve",
		"dev:progress": "pnpm run --silent dev --progress",
		"lint": "eslint --max-warnings 0 .",
		"lint:fix": "eslint --fix .",
		"type-check": "pnpm run '/type-check:.*/'",
		"type-check:test": "tsc -p ./test/tsconfig.json --noEmit",
		"type-check:test-setup": "tsc -p ./test/tsconfig.setup.json --noEmit",
		"type-check:src": "tsc -p ./tsconfig.json --noEmit",
		"type-check:webpack": "tsc -p ./tsconfig.webpack.json --noEmit",
		"webpack:build": "cross-env NODE_OPTIONS=\"--no-warnings=ExperimentalWarning --experimental-strip-types\" webpack --disable-interpret -c ./webpack.config.ts --env prod --fail-on-warnings",
		"webpack:serve": "cross-env NODE_OPTIONS=\"--no-warnings=ExperimentalWarning --experimental-strip-types\" webpack serve --disable-interpret -c ./webpack.config.ts",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"start": "pnpm dev"
	},
	"dependencies": {
		"@hcaptcha/react-hcaptcha": "1.12.0",
		"bowser": "2.11.0",
		"classnames": "2.5.1",
