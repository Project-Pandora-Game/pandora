{
	"name": "pandora-client-web",
	"version": "0.1.0",
	"private": true,
	"repository": "git@github.com:Project-Pandora-Game/pandora-client-web.git",
	"license": "LGPL-3.0-or-later",
	"type": "module",
	"browserslist": [
		"> 0.5%, last 2 versions and last 2 years, Firefox ESR, not dead"
	],
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "pnpm run --silent type-check:webpack && pnpm run webpack:build",
		"dev": "pnpm run --silent type-check:webpack && pnpm run webpack:serve",
		"dev:progress": "pnpm run --silent dev --progress",
		"lint": "eslint --max-warnings 0 .",
		"lint:fix": "eslint --fix .",
		"type-check": "pnpm run '/type-check:.*/'",
		"type-check:test": "tsc -p ./test/tsconfig.json --noEmit",
		"type-check:test-setup": "tsc -p ./test/tsconfig.setup.json --noEmit",
		"type-check:src": "tsc -p ./tsconfig.json --noEmit",
		"type-check:webpack": "tsc -p ./tsconfig.webpack.json --noEmit",
		"webpack:build": "cross-env NODE_OPTIONS=\"--no-warnings=ExperimentalWarning --experimental-strip-types\" webpack --disable-interpret -c ./webpack.config.ts --env prod --fail-on-warnings",
		"webpack:serve": "cross-env NODE_OPTIONS=\"--no-warnings=ExperimentalWarning --experimental-strip-types\" webpack serve --disable-interpret -c ./webpack.config.ts",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"start": "pnpm dev"
	},
	"dependencies": {
		"@hcaptcha/react-hcaptcha": "2.0.2",
		"bowser": "2.13.1",
		"classnames": "2.5.1",
		"client-zip": "2.5.0",
		"color-parse": "2.0.2",
		"history": "5.3.0",
		"immer": "11.1.3",
		"inter-ui": "4.1.1",
		"lodash-es": "4.17.23",
		"lz-string": "1.5.0",
		"nanoid": "5.1.6",
		"pandora-common": "workspace:*",
		"pixi-filters": "6.1.5",
		"pixi-viewport": "6.0.3",
		"pixi.js": "8.16.0",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"react-hook-form": "7.71.1",
		"react-reconciler": "0.33.0",
		"react-reverse-portal": "2.3.0",
		"react-rnd": "10.5.2",
		"react-router": "7.13.0",
		"react-switch": "7.1.0",
		"react-toastify": "10.0.6",
		"socket.io-client": "4.8.3",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@jest/globals": "30.2.0",
		"@pmmmwh/react-refresh-webpack-plugin": "0.6.2",
		"@testing-library/jest-dom": "6.9.1",
		"@testing-library/react": "16.3.2",
		"@testing-library/user-event": "14.6.1",
		"@types/jest": "30.0.0",
		"@types/lodash-es": "4.17.12",
		"@types/node": "24.10.12",
		"@types/offscreencanvas": "2019.7.3",
		"@types/penner": "0.1.3",
		"@types/react": "19.2.13",
		"@types/react-dom": "19.2.3",
		"@types/react-reconciler": "0.33.0",
		"@types/webpack": "5.28.5",
		"autoprefixer": "10.4.24",
		"copy-webpack-plugin": "13.0.1",
		"cross-env": "10.1.0",
		"css-loader": "7.1.3",
		"file-loader": "6.2.0",
		"fork-ts-checker-webpack-plugin": "9.1.0",
		"html-webpack-plugin": "5.6.6",
		"jest": "30.2.0",
		"jest-environment-jsdom": "30.2.0",
		"mini-css-extract-plugin": "2.10.0",
		"postcss": "8.5.6",
		"postcss-loader": "8.2.0",
		"react-refresh": "0.18.0",
