{
	"name": "pandora-tests",
	"version": "0.1.0",
	"description": "Collection of End to End tests for Pandora",
	"private": true,
	"license": "GPL-3.0-or-later",
	"scripts": {
		"lint": "eslint --max-warnings 0 --report-unused-disable-directives .",
		"lint:fix": "eslint --fix .",
		"type-check": "pnpm run '/type-check:.*/'",
		"type-check:test-e2e": "tsc -p ./test/tsconfig.json --noEmit",
		"test-e2e": "jest",
		"test-e2e:coverage": "jest --coverage",
		"pandora-server-directory": "pandora-server-directory"
	},
	"devDependencies": {
		"@types/express": "4.17.17",
		"@types/jest": "29.5.3",
		"@types/node": "18.17.5",
		"expect-puppeteer": "9.0.0",
		"express": "4.18.2",
		"jest": "29.6.2",
		"jest-dev-server": "9.0.0",
		"jest-environment-node": "29.6.2",
		"pandora-common": "workspace:*",
		"pandora-server-directory": "workspace:*",
		"pandora-server-shard": "workspace:*",
		"puppeteer": "21.0.3",
		"rimraf": "5.0.1",
		"ts-jest": "29.1.1",
		"tslib": "2.6.1",
		"typescript": "5.1.6"
	}
}
